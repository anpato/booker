    "express": "^4.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.5",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
